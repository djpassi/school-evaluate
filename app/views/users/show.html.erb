<div class="container">
      <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad" >
          <div class="panel panel-info">
            <div class="panel-heading">
              <h3 class="panel-title"><%= @user.name %></h3>
            </div>
            <div class="panel-body">
              <div class="row">
                <div class="col-md-3 col-lg-3 " align="center"> <img alt="User Pic" src="http://www.pieglobal.com/wp-content/uploads/2015/10/placeholder-user.png" class="img-circle img-responsive"> </div>

                <div class=" col-md-9 col-lg-9 ">
                  <table class="table table-user-information">
                    <tbody>
                      <tr>
                        <td>Profesor de:</td>
                        <td>????</td>
                      </tr>

                      <tr>
                        <td>Email</td>
                        <td><a href="mailto:<%= @user.email%>"><%= @user.email %></a></td>
                    </tbody>
                  </table>

                </div>
              </div>
            </div>
                <div class="panel-footer">
                  <% if @final_skills%>


                     <h1><span class="label label-primary"><%=  @final_skills[0][0]%></span></h1>
                     <h1><span class="label label-success"><%=  @final_skills[1][0]%></span></h1>



                 <%else%>
                 <h4>No se presentan evaluaciones actualmente para el profesor.</h4>

                  <%end%>

                 </div>

        </div>
      </div>
    </div>
